{
  "extends": [
    "config:base",
    "github>Omochice/renovate-config:deno"
  ],
  "timezone": "Asia/Tokyo",
  "dependencyDashboard": false,
  "automerge": true,
  "branchConcurrentLimit": 0,
  "packageRules": [
    {
      "matchFileNames": ["**/deps.ts"],
      "groupName": "deno dependencies."
    },
    {
      "matchFileNames": ["package.json"],
      "groupName": "package.json dependencies."
    }
  ]
}
