{
  "textlint off on": {
    "scope": "markdown",
    "prefix": ["textlint-off-on", "tloo"],
    "body": [
      "<!-- textlint-disable -->",
      "$0",
      "<!-- textlint-enable -->"
    ],
    "description": "Stop textlint rule for a time."
  },
  "comment": {
    "scope": "markdown",
    "prefix": "comment",
    "body": [
      "<!-- $0 -->"
    ],
    "description": "Comment"
  },
  "break": {
    "scope": "markdown",
    "prefix": "br",
    "body": [
      "<br>$0"
    ],
    "description": "Make br tag."
  }
}
